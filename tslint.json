{
    "extends": ["tslint-config-standard", "tslint-config-prettier"],
    "rules": {
        "no-any": true,
        "semicolon": [
            true,
            "always"
        ],
        "ter-indent": [
            true,
            4,
            { "SwitchCase": 1 }
        ],
        "adjacent-overload-signatures": true,
        "member-access": [
            true,
            "no-public"
        ],
        "no-console": [true],
        "no-debugger": true
    }
}