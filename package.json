{
  "name": "mibi-portal-server",
  "version": "3.0.1",
  "license": "MIT",
  "scripts": {
    "start": "node ./lib/main.js",
    "serve": "npx nodemon -e ts --exec \"npm run serve:pipe\"",
    "debug": "npx nodemon -e ts --exec \"npm run debug:pipe\"",
    "serve:pipe": "npm run build && npm run test && node ./lib/main.js",
    "debug:pipe": "npm run build && node --inspect-brk ./lib/main.js",
    "start:admin-util": "cp ./config/admin-util/config.js ./node_modules/mongo-express/config.js && node ./node_modules/mongo-express/app.js",
    "dependency-check:production": "npx npm-auditor-ci --threshold low -ignoreDev",
    "dependency-check:qa": "npx npm-auditor-ci --threshold low",
    "cp:view": "npx copyfiles -u 1 ./src/**/views/**/* ./lib",
    "cp:json": "npx copyfiles -u 1 ./src/**/*.json ./lib",
    "cp:scripts": "npx copyfiles -u 1 ./src/**/*.sh ./lib",
    "cp:all": "npm run cp:view && npm run cp:scripts && npm run cp:json",
    "tsc": "tsc",
    "build": "npm run tsc && npm run cp:all",
    "prebuild": "npm run clean",
    "test": "jest --config=jest.json",
    "test:integration": "jest --config=jest-integration.json --verbose",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage && open coverage/lcov-report/index.html",
    "test:report": "npm test -- --reporters default jest-stare --testResultsProcessor=jest-stare && open jest-stare/index.html",
    "clean": "npx rimraf lib/*",
    "lint": "tslint --project tsconfig.json --fix -c tslint.json 'src/**/*.ts'",
    "pretty": "prettier --write \"src/**/*.ts\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "./hooks/commit-msg ./.git/COMMIT_EDITMSG"
    }
  },
  "lint-staged": {
    "linters": {
      "src/**/*.ts": [
        "npm run pretty",
        "npm run lint",
        "git add"
      ]
    },
    "ignore": [
      "src/**/__mocks__/*.ts",
      "src/**/__tests__/*.ts"
    ]
  },
  "private": true,
  "dependencies": {
    "app-root-dir": "^1.0.2",
    "argon2": "^0.26.1",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.1",
    "compression": "^1.7.3",
    "config": "^1.29.4",
    "core-js": "^2.5.3",
    "cors": "^2.8.4",
    "express": "^4.15.4",
    "express-jwt": "^5.3.0",
    "fast-csv": "^2.4.1",
    "filereader": "^0.10.3",
    "fs-extra": "^6.0.1",
    "fs-readfile-promise": "^3.0.1",
    "fuse.js": "3.2.1",
    "handlebars": "^4.7.6",
    "helmet": "^3.21.1",
    "html5-history-api": "^4.2.8",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.3.2",
    "jest-stare": "^1.18.1",
    "jquery": "^3.5.0",
    "jsonwebtoken": "^8.1.0",
    "linebyline": "^1.3.0",
    "lodash": "^4.17.14",
    "moment": "^2.20.1",
    "mongo-express": "^0.51.2",
    "mongodb": "^3.3.3",
    "mongoose": "^5.4.10",
    "mongoose-unique-validator": "^2.0.1",
    "morgan": "^1.9.1",
    "multer": "^1.3.0",
    "nodemailer": "^4.4.1",
    "pdfmake": "^0.1.59",
    "processenv": "^1.1.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "rxjs": "^6.2.1",
    "swagger-ui-express": "^4.0.2",
    "validate.js": "^0.12.0",
    "winston": "^3.0.0",
    "xlsx": "^0.14.1",
    "xlsx-populate": "^1.19.1"
  },
  "devDependencies": {
    "@types/app-root-dir": "^0.1.0",
    "@types/bluebird": "^3.5.20",
    "@types/body-parser": "^1.16.8",
    "@types/compression": "0.0.36",
    "@types/config": "0.0.34",
    "@types/cors": "^2.8.3",
    "@types/express": "^4.11.1",
    "@types/express-jwt": "^0.0.40",
    "@types/fs-readfile-promise": "^3.0.0",
    "@types/helmet": "0.0.42",
    "@types/jest": "^24.0.12",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/lodash": "^4.14.104",
    "@types/mongoose": "^5.4.10",
    "@types/mongoose-unique-validator": "^1.0.1",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.6",
    "@types/node": "^10.3.6",
    "@types/nodemailer": "^4.3.4",
    "@types/reflect-metadata": "^0.1.0",
    "@types/request": "^2.48.3",
    "@types/request-promise-native": "^1.0.17",
    "@types/swagger-ui-express": "^3.0.0",
    "@types/xlsx": "0.0.36",
    "copyfiles": "^2.1.1",
    "husky": "^1.1.3",
    "jest": "^25.3.0",
    "lint-staged": "^8.0.4",
    "mock-express-request": "^0.2.2",
    "mock-express-response": "^0.2.2",
    "nodemon": "^2.0.3",
    "prettier": "^1.4.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.0.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^3.4.0"
  },
  "mibiConfig": {
    "lastChange": "2019-04-16 11:25:17 +0200"
  }
}
