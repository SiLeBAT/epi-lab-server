{
  "name": "mibi-portal-server",
  "version": "3.0.1",
  "license": "MIT",
  "scripts": {
    "start": "node ./lib/main.js",
    "serve": "npx nodemon -e ts --exec \"npm run serve:pipe\"",
    "debug": "npx nodemon -e ts --exec \"npm run debug:pipe\"",
    "serve:pipe": "npm run build && npm run test && node ./lib/main.js",
    "debug:pipe": "npm run build && node --inspect-brk ./lib/main.js",
    "start:admin-util": "cp ./config/admin-util/config.js ./node_modules/mongo-express/config.js && node ./node_modules/mongo-express/app.js",
    "cp:view": "npx copyfiles -u 1 ./src/**/views/**/* ./lib",
    "cp:json": "npx copyfiles -u 1 ./src/**/*.json ./lib",
    "cp:scripts": "npx copyfiles -u 1 ./src/**/*.sh ./lib",
    "cp:all": "npm run cp:view && npm run cp:scripts && npm run cp:json",
    "tsc": "tsc",
    "build": "npm run tsc && npm run cp:all",
    "prebuild": "npm run clean",
    "test": "jest --config=jest.json",
    "test:integration": "jest --config=jest-integration.json --verbose",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage && open coverage/lcov-report/index.html",
    "test:report": "npm test -- --reporters default jest-stare --testResultsProcessor=jest-stare && open jest-stare/index.html",
    "clean": "npx rimraf lib/*",
    "lint": "tslint --project tsconfig.json --fix -c tslint.json \"src/**/*.ts\"",
    "pretty": "prettier --write \"src/**/*.ts\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "./hooks/commit-msg ./.git/COMMIT_EDITMSG"
    }
  },
  "lint-staged": {
    "linters": {
      "src/**/*.ts": [
        "npm run pretty",
        "npm run lint",
        "git add"
      ]
    },
    "ignore": [
      "src/**/__mocks__/*.ts",
      "src/**/__tests__/*.ts"
    ]
  },
  "private": true,
  "dependencies": {
    "argon2": "0.26.2",
    "bluebird": "~3.7.2",
    "compression": "~1.7.4",
    "config": "~3.3.1",
    "cors": "~2.8.5",
    "express": "~4.17.1",
    "express-jwt": "^5.3.3",
    "fast-csv": "~4.3.1",
    "fs-readfile-promise": "~3.0.1",
    "fuse.js": "^6.4.1",
    "handlebars": "~4.7.6",
    "helmet": "~3.23.3",
    "inversify": "~5.0.1",
    "inversify-express-utils": "~6.3.2",
    "jsonwebtoken": "~8.5.1",
    "lodash": "~4.17.19",
    "moment": "~2.27.0",
    "mongo-express": "0.53.0",
    "mongoose": "~5.9.27",
    "mongoose-unique-validator": "~2.0.3",
    "morgan": "~1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "6.4.11",
    "pdfmake": "0.1.68",
    "reflect-metadata": "0.1.13",
    "request": "~2.88.0",
    "request-promise-native": "1.0.9",
    "swagger-ui-express": "~4.1.4",
    "validate.js": "0.13.1",
    "winston": "~3.3.3",
    "xlsx": "0.16.5",
    "xlsx-populate": "~1.21.0"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.32",
    "@types/compression": "1.7.0",
    "@types/config": "0.0.36",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.2",
    "@types/express-jwt": "0.0.42",
    "@types/fs-readfile-promise": "3.0.0",
    "@types/helmet": "0.0.47",
    "@types/jest": "24.9.1",
    "@types/jsonwebtoken": "8.5.0",
    "@types/lodash": "4.14.158",
    "@types/mongoose": "5.7.34",
    "@types/mongoose-unique-validator": "1.0.4",
    "@types/morgan": "1.9.1",
    "@types/multer": "1.4.3",
    "@types/node": "12.12.53",
    "@types/nodemailer": "4.6.8",
    "@types/request": "2.48.5",
    "@types/request-promise-native": "1.0.17",
    "@types/swagger-ui-express": "3.0.1",
    "copyfiles": "~2.3.0",
    "husky": "~4.2.5",
    "jest": "~25.5.4",
    "jest-stare": "~1.27.0",
    "lint-staged": "~8.2.1",
    "mock-express-request": "0.2.2",
    "mock-express-response": "0.2.2",
    "nodemon": "~2.0.4",
    "prettier": "1.19.1",
    "rimraf": "~3.0.2",
    "ts-jest": "~24.3.0",
    "tslint": "~6.1.3",
    "tslint-config-prettier": "~1.18.0",
    "tslint-config-standard": "9.0.0",
    "typescript": "3.9.7"
  },
  "mibiConfig": {
    "lastChange": "2019-04-16 11:25:17 +0200"
  }
}
